      "return_ty": {
        "Option": "String"
      }
    },
    {
      "name": "keys_count",
      "is_mutable": false,
      "args": [],
      "return_ty": "U32"
    },
    {
      "name": "all_variables",
      "is_mutable": false,
      "args": [],
      "return_ty": {
        "Map": {
          "key": "String",
          "value": {
            "List": "U8"
          }
        }
      }
    },
    {
