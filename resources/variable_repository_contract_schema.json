      "is_mutable": true,
      "args": [
        {
          "name": "key",
          "ty": "String"
        },
        {
          "name": "value",
          "ty": {
            "List": "U8"
          }
        },
        {
          "name": "activation_time",
          "ty": {
            "Option": "U64"
          }
        }
      ],
      "return_ty": "Unit"
    },
    {
