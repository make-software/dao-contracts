  "events": [
    {
      "name": "ValueUpdated",
      "fields": [
        {
          "name": "key",
          "ty": "String"
        },
        {
          "name": "value",
          "ty": {
            "List": "U8"
          }
        },
        {
          "name": "activation_time",
          "ty": {
            "Option": "U64"
          }
        }
