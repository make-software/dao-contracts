        "Option": "Key"
      }
    },
    {
      "name": "change_ownership",
      "is_mutable": true,
      "args": [
        {
          "name": "owner",
          "ty": "Key"
        }
      ],
      "return_ty": "Unit"
    },
    {
      "name": "add_to_whitelist",
      "is_mutable": true,
      "args": [
        {
          "name": "address",
          "ty": "Key"
        }
      ],
      "return_ty": "Unit"
    },
    {
      "name": "remove_from_whitelist",
      "is_mutable": true,
      "args": [
        {
          "name": "address",
          "ty": "Key"
        }
      ],
      "return_ty": "Unit"
    }
